<Application x:Class="Diplom.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         
    	<SolidColorBrush x:Key="Бэкграунд" Color="#FFDFE0DA"/>
    	<SolidColorBrush x:Key="Граница" Color="#FF555B5E"/>
    	<LinearGradientBrush x:Key="Кнопка" EndPoint="0,1" StartPoint="0,0">
    		<GradientStop Color="White" Offset="1"/>
    		<GradientStop Color="White"/>
    		<GradientStop Color="#FF94C07C" Offset="0.392"/>
    		<GradientStop Color="White" Offset="0.19"/>
    	</LinearGradientBrush>
    	<LinearGradientBrush x:Key="Поле заполнения" EndPoint="0.5,1" StartPoint="0.5,0">
    		<GradientStop Color="Silver" Offset="0.013"/>
    		<GradientStop Color="Silver" Offset="0.987"/>
    		<GradientStop Color="White" Offset="0.754"/>
    		<GradientStop Color="White" Offset="0.22"/>
    	</LinearGradientBrush>
    	<RadialGradientBrush x:Key="Бекграунд_зеленый">
    		<GradientStop Color="#FFFDFDFD" Offset="0.47"/>
    		<GradientStop Color="#FFE0F7CE" Offset="1"/>
    	</RadialGradientBrush>
         
    </Application.Resources>
</Application>
